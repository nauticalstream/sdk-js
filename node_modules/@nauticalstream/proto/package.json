{
  "name": "@nauticalstream/proto",
  "version": "1.0.1",
  "description": "Protocol Buffer definitions for Nauticalstream services",
  "type": "module",
  "exports": {
    "./*": "./gen/ts/*.js"
  },
  "scripts": {
    "buf:lint": "buf lint",
    "buf:generate": "buf generate",
    "buf:breaking": "buf breaking --against '.git#branch=main'",
    "clean": "rm -rf gen/*",
    "build": "npm run buf:generate"
  },
  "keywords": [
    "protobuf",
    "proto",
    "grpc"
  ],
  "files": [
    "gen/ts",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nauticalstream/proto.git"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.11.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.65.0",
    "@bufbuild/protoc-gen-es": "^2.11.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^25.0.3"
  }
}
